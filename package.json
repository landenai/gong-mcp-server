{
  "name": "gong-mcp-server",
  "version": "1.0.0",
  "description": "MCP server for querying Gong calls, transcripts, deals, and account activity",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "vercel-build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "dev:vercel": "vercel dev",
    "inspect": "npx @anthropic-ai/mcp-inspector",
    "test-ui": "tsx test-ui.ts",
    "test": "tsx comprehensive-test.ts",
    "deploy:preview": "vercel",
    "deploy:prod": "vercel --prod"
  },
  "dependencies": {
    "@google-cloud/secret-manager": "^5.0.0",
    "@modelcontextprotocol/sdk": "^1.0.0",
    "@vercel/node": "^3.0.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  }
}
