# Gong API Credentials
# Get these from: https://app.gong.io/settings/api/documentation
GONG_ACCESS_KEY=your_gong_access_key_here
GONG_ACCESS_KEY_SECRET=your_gong_access_key_secret_here

# Authentication
ALLOWED_EMAIL_DOMAINS=sentry.io,getsentry.com

# Google OAuth (for /api/auth flow)
# Get these from: https://console.cloud.google.com/apis/credentials
GOOGLE_CLIENT_ID=your_client_id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your_client_secret

# Token secret (for signing API tokens)
# Generate with: openssl rand -base64 32
TOKEN_SECRET=your_random_secret_here
